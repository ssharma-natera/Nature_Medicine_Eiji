ggsurvplot(KM_curve, data = cir_data, pval = TRUE, conf.int = TRUE,risk.table = TRUE,xlab="Time(months)", ylab="Disease Free Survival", palette = c("red", "blue"), break.time.by=3,xlim = c(0,24))
